<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="feng.jiyun.com.myapplication.MainActivity">

   <ImageView
       android:id="@+id/image1"
       android:src="@drawable/logo_qq"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginStart="18dp"
       android:layout_marginTop="20dp"
       android:layout_alignParentTop="true"
       android:layout_alignParentStart="true" />
    <ImageView
        android:id="@+id/image2"
        android:src="@drawable/logo_qq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="41dp"
        android:layout_alignTop="@+id/image1"
        android:layout_toEndOf="@+id/image1" />

    <ImageView
        android:id="@+id/image3"
        android:src="@drawable/logo_qq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/image1"
        android:layout_alignStart="@+id/image1"
        android:layout_marginTop="31dp" />
    <ImageView
        android:id="@+id/image4"
        android:src="@drawable/logo_qq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/image3"
        android:layout_alignStart="@+id/image2" />

    <ImageView
        android:id="@+id/image5"
        android:src="@drawable/logo_qq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="43dp"
        android:layout_below="@+id/image3"
        android:layout_alignStart="@+id/image3" />
    <ImageView
        android:id="@+id/image6"
        android:src="@drawable/logo_qq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/image5"
        android:layout_alignStart="@+id/image4" />

   <tyrantgit.widget.HeartLayout
       android:id="@+id/hrart"
       android:layout_alignParentRight="true"
       android:layout_width="100dp"
       android:layout_height="match_parent" />

</RelativeLayout>
